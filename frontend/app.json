{
  "expo": {
    "name": "MentIQ",
    "slug": "mentiq",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "scheme": "elearningapp",
    "userInterfaceStyle": "automatic",
        "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select images for announcements.",
        "NSCameraUsageDescription": "This app needs access to your camera to take photos for announcements.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save images to your photo library.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.adi0902.mentiq"
    },
    "android": {
      "adaptiveIcon": {
      "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/Logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.adi0902.mentiq"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to save downloaded images.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save images to your photo library.",
          "isAccessMediaLocationEnabled": true,
          "audioPermission": "Allow $(PRODUCT_NAME) to access your audio library for video downloads."
        }
      ],
      "expo-video",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "060877e6-0f91-477b-ac5a-65db05c0acfb"
      }
    },
    "owner": "adi_0902",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/060877e6-0f91-477b-ac5a-65db05c0acfb"
    }
  }
}
